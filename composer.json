        "roave/security-advisories": "dev-master",
        "psr/container": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^2.8",
        "filp/whoops": "^1.1 || ^2.0",
        "satooshi/php-coveralls": "^1.0",
        "phpunit/php-code-coverage": "^5.0",
        "phpmetrics/phpmetrics": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Mps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MpsTest\\": "test/"
        }
    }
}
